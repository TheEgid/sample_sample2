generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma-client"
  binaryTargets   = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL_DEV")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
