datasource db {
  provider = "sqlite"
  url      = "file:./database-sql-lite.db"
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma-client"
  binaryTargets = ["native", "debian-openssl-3.0.x", "linux-musl-openssl-3.0.x"]
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String?
}
