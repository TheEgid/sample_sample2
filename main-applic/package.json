{
    "$schema": "http://json.schemastore.org/package",
    "name": "noname",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "upgrade-packages": "pnpm up --latest",
        "select-schema": "pnpm tsx ./prisma/select-schema.ts",
        "start_dev": "pnpm select-schema && next dev --port 3006 --turbo",
        "start": "next start -p 3007",
        "stylelint": "stylelint \"**/*.scss\" --fix",
        "frmt": "eslint . --ignore-pattern \"build/*\" --fix",
        "build": "pnpm select-schema && next build",
        "test": "jest",
        "serve": "pnpm build && pnpm start",
        "prisma:generate": "prisma generate",
        "prisma-push": "pnpm prisma db push",
        "prisma-studio": "pnpm prisma studio"
    },
    "dependencies": {
        "@mantine/core": "^8.1.3",
        "@mantine/hooks": "^8.1.3",
        "@prisma/client": "^6.11.1",
        "@prisma/engines": "^6.11.1",
        "dotenv": "^17.1.0",
        "effector": "^23.4.1",
        "effector-react": "^23.3.0",
        "ky": "^1.8.1",
        "next": "15.3.5",
        "postcss": "^8.5.6",
        "postcss-preset-mantine": "^1.18.0",
        "postcss-simple-vars": "^7.0.1",
        "prisma": "^6.11.1",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-toastify": "^11.0.5"
    },
    "devDependencies": {
        "@eslint/compat": "^1.3.1",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.30.1",
        "@next/eslint-plugin-next": "^15.3.5",
        "@stylistic/eslint-plugin": "^5.1.0",
        "@stylistic/stylelint-config": "^2.0.0",
        "@stylistic/stylelint-plugin": "^3.1.3",
        "@types/jest": "^30.0.0",
        "@types/node": "24.0.11",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "eslint": "^9.30.1",
        "eslint-config-next": "15.3.5",
        "eslint-plugin-effector": "^0.15.0",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "jest": "^30.0.4",
        "reset-css": "^5.0.2",
        "sass": "^1.89.2",
        "stylelint": "^16.21.1",
        "stylelint-config-standard-scss": "^15.0.1",
        "stylelint-order": "^7.0.0",
        "stylelint-order-config-standard": "^0.1.3",
        "stylelint-scss": "^6.12.1",
        "tsx": "^4.20.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.36.0"
    },
    "prisma": {
        "schema": "./prisma/schema.prisma"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@parcel/watcher",
            "@prisma/client",
            "@prisma/engines",
            "esbuild",
            "prisma",
            "sharp",
            "unrs-resolver"
        ]
    }
}
